{
  "extends": ["airbnb", "plugin:@next/next/recommended"],
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "@next/next/no-img-element": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/no-array-index-key": 0,
    "react/no-danger": 0,
    "react/prop-types": 0
  }
}
